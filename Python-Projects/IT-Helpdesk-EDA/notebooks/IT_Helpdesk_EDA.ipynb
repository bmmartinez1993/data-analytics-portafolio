date_cols = ['Created Date', 'Resolved Date']

for col in date_cols:
    df[col] = pd.to_datetime(df[col], errors='coerce')

df['Resolution Time (hrs)'] = (df['Resolved Date'] - df['Created Date']).dt.total_seconds() / 3600

df['Month'] = df['Created Date'].dt.month_name()
df['Weekday'] = df['Created Date'].dt.day_name()
df['Hour'] = df['Created Date'].dt.hour
df['Week'] = df['Created Date'].dt.isocalendar().week

df['SLA Met'] = np.where(df['Resolution Time (hrs)'] <= df['SLA Target (hrs)'], 'Yes', 'No')
